# ========================================
# RPC URLs
# ========================================
MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY

# ========================================
# Private key for deployment (without 0x prefix)
# ========================================
PRIVATE_KEY=

# ========================================
# Etherscan API key for contract verification
# ========================================
ETHERSCAN_API_KEY=

# ========================================
# Enable forking for local testing
# ========================================
FORKING=false

# ========================================
# Deployment Configuration
# ========================================

# USDC Token Addresses (official Circle addresses)
# Choose based on your deployment network

# Sepolia testnet USDC
SEPOLIA_USDC_ADDRESS=0x1c7D4B196Cb0C7B01d743Fbc6116a902379C7238

# Ethereum mainnet USDC
MAINNET_USDC_ADDRESS=0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48

# Admin address (manages roles, strategies, limits)
# If not set, deployer address will be used
ADMIN_ADDRESS=

# ========================================
# Vault Configuration
# ========================================

# Vault token name
VAULT_NAME=Tesseract Demo Vault

# Vault token symbol
VAULT_SYMBOL=TDV

# Profit unlock time in seconds
# Default: 604800 (7 days)
PROFIT_MAX_UNLOCK_TIME=604800

# ========================================
# Vault Limits
# ========================================

# Maximum total deposits (in USDC, without decimals)
# Default: 10000000 (10M USDC)
MAX_DEPOSIT_LIMIT=10000000
